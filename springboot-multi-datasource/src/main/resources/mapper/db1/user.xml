<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jadson.demo.db1.mapper.UserMapper">

    <select id="selectList" resultType="com.jadson.demo.db1.model.User">
        select
          id as id,
          name as name,
          age as age
        from t_user
    </select>
</mapper>