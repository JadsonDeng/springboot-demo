<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jadson.demo.db2.mapper.StudentMapper">

    <select id="selectList" resultType="com.jadson.demo.db2.model.Student">
        select
          id as id,
          name as name,
          stu_no as stuNo,
          sex as sex
        from student
    </select>
</mapper>